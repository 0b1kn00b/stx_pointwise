package stx.fn.pack;

@:callable abstract VBlock<T>(VBlockDef<T>) from VBlockDef<T>{
  static public function unit<T>():VBlock<T>{
    return () -> {}
  }
  public function new(self:VBlockDef<T>){
    this = self;
  }
  // @:from static public function fromBlock<T>(b:Block):VBlock<T>{
  //   return new VBLokb;
  // }
  @:to public function toBlock():Block{
    return this;
  }
  @:to public function toBlockT():Void->Void{
    return this;
  }
}